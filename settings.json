{
  "workbench.colorTheme": "One Dark Pro Night Flat",
  "editor.fontFamily": "'Dank Mono', 'JetBrains Mono', 'Fira Code', Consolas, 'Courier New', monospace",
  "editor.wordWrap": "on",
  "glassit.alpha": 248,
  "workbench.activityBar.location": "left",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "workbench.iconTheme": "material-icon-theme",
  "material-icon-theme.hidesExplorerArrows": true,
  "editor.formatOnSave": true,
  "files.autoSave": "afterDelay",
  "workbench.productIconTheme": "fluent-icons",
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
    "*.sqlite": "${capture}.${extname}-*",
    "*.db": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.s3db": "${capture}.${extname}-*"
  },
  "chat.commandCenter.enabled": false,
  "animations.Enabled": true,
  "workbench.list.smoothScrolling": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "liveServer.settings.donotShowInfoMsg": true,
  "editor.codeLensFontFamily": "Dank Mono",
  "editor.inlayHints.fontFamily": "Dank Mono",
  "debug.console.fontFamily": "Dank Mono",
  "scm.inputFontFamily": "Dank Mono",
  "editor.inlineSuggest.fontFamily": "Dank Mono",
  "terminal.integrated.fontFamily": "Dank Mono",
  "notebook.markup.fontFamily": "Dank Mono",
  "notebook.output.fontFamily": "Dank Mono",
  "chat.editor.fontFamily": "Dank Mono",
  "editor.fontSize": 15,
  "editor.fontLigatures": true,
  "editor.fontWeight": "600",
  "editor.minimap.enabled": false,
  "editor.stickyScroll.enabled": false,
  "settingsSync.ignoredExtensions": ["moalamri.inline-fold"],
  "editor.linkedEditing": true,
  "window.commandCenter": false,
  "breadcrumbs.enabled": false,
  "workbench.editor.showTabs": "single",
  "explorer.confirmDragAndDrop": false,
  "terminal.integrated.fontWeight": "700",
  "chat.instructionsFilesLocations": {
    ".github/instructions": true,
    "/tmp/postman-http-request-post-response.instructions.md": true,
    "/tmp/postman-http-request-pre-request.instructions.md": true,
    "/tmp/postman-collections-post-response.instructions.md": true,
    "/tmp/postman-collections-pre-request.instructions.md": true,
    "/tmp/postman-folder-post-response.instructions.md": true,
    "/tmp/postman-folder-pre-request.instructions.md": true
  },
  "editor.quickSuggestions": {
    "other": "on",
    "comments": "off",
    "strings": "off"
  },
  "editor.lineNumbers": "relative",
  "editor.cursorBlinking": "solid",
  "editor.cursorStyle": "block",
  "editor.scrollbar.horizontal": "visible",
  "editor.scrollbar.vertical": "visible",
  "window.menuBarVisibility": "hidden",
  "explorer.confirmDelete": false,
  "workbench.statusBar.visible": false,
  "workbench.colorCustomizations": {
    "statusBar.background": "#9f00a6",
    "statusBar.noFolderBackground": "#8a008f",
    "statusBar.debuggingBackground": "#8a008f",
    "statusBar.foreground": "#ffffff",
    "statusBar.debuggingForeground": "#ffffff"
  }
}
